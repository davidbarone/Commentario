  "version": "0.2.0",
  "configurations": [
    {
      "name": "C#: Dbarone.Net.CommentarioConsole Debug",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/Server/Dbarone.Net.CommentarioConsole/bin/Debug/net8.0/Dbarone.Net.CommentarioConsole.dll",
      //"args": "-h"
      "args": "${workspaceFolder}/Server/ExampleLibrary/bin/Debug/net8.0/ExampleLibrary.dll ${workspaceFolder}/Server/ExampleLibrary/ExampleLibrary.html -t html -r ${workspaceFolder}/Server/ExampleLibrary/readme.txt",
      "preLaunchTask": "Build: debug"
    },
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}/Client"],
      "outFiles": ["${workspaceFolder}/Client/out/**/*.js"],
      "preLaunchTask": "Extension PreBuild Task"
    }
  ]
}
